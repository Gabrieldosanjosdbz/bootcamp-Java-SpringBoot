	}

	@Override
	public void atualizarPagina() {
		System.out.println("Pagina atualizada!");
		
	}

	@Override
	public void adicionarNovaAba() {
		System.out.println("");
		
	}

	@Override
	public void exibirPagina(String url) {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void ligar() {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void atender() {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void iniciarCorreioVoz() {
		// TODO Auto-generated method stub
		
	}

	
}


